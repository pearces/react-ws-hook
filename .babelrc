{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "env": {
    "node": {
      "presets": [
        ["@babel/preset-env", { "targets": { "node": "14" } }]
      ]
    },
    "module": {
      "presets": [
        ["@babel/preset-env", { "modules": false }]
      ],
      "plugins": [
        ["@babel/plugin-transform-runtime", { "useESModules": true }]
      ]
    },
    "test": {
      "presets": [
        ["@babel/preset-env", { "targets": { "esmodules": false } }]
      ],
      "plugins": [
        ["@babel/plugin-transform-runtime", { "regenerator": true }]
      ]
    }
  }
}
